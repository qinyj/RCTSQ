extends layout
block content
  main.main
    section.intro
        h3.field.regheader #{title}
        form.regform(method='post')
            p.mess #{loginmsg}
            div.item
                label.label(for='userid') UserID
                input(type='text',placeholder='input your userid',id='userid',name='userid')
            div.item
                label.label(for='password') Password
                input(type='password',placeholder='password',id='password',name='password')
            div.item
                label.label
                input(type='submit' id='sub',name='sub' value='Login')
